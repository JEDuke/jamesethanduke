@{
    ViewData["Title"] = "James Ethan Duke";
}
<div id="divMainBackground" class="container mainBackground">

</div>
<script src="/lib/jquery/dist/jquery.js"></script>
<script type="text/javascript">
    $( document ).ready(function() {
        var i = 1;
        setInterval(() => {
            i < 3 ? i++ : i=1;
            setBackgroundImage(i);
        }, 20000);
    });

    function setBackgroundImage(x){
        var backgroundImageUrl = "/images/homepage" + x + ".jpeg";
        $(document.getElementById("divMainBackground")).css("background-image", "url(" + backgroundImageUrl + ")");
    }

</script>